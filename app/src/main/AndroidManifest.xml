<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetonutri">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetoNutri">
        <activity
            android:name=".RecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name=".VitaminasActivity"
            android:exported="false"
            android:label="@string/title_activity_vitaminas"
            android:theme="@style/Theme.ProjetoNutri.NoActionBar" />
        <activity
            android:name=".View.fragments.EditPerfilActivity"
            android:exported="false" />

        <service
            android:name=".Service.Notificacao"
            android:exported="false" />

        <activity
            android:name=".View.fragments.SaibaMaisActivity"
            android:exported="false" />
        <activity
            android:name=".View.fragments.HomeActivicy"
            android:exported="false" />
        <activity
            android:name=".View.fragments.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
